    "@types/swagger-ui-react": "^3.23.1",
    "axios": "^0.19.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "swagger-ui-react": "^3.25.0",
    "sweetalert2": "^9.8.2",
    "ts-jest": "^25.2.1",
